<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Projet2\PlatformBundle\Entity\Club" table="club">
    <id name="idclub" type="integer" column="idclub">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="club_idclub_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nomClub" type="string" column="nom_club" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="localisation" type="string" column="localisation" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomdirigeant" type="string" column="nomdirigeant" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
